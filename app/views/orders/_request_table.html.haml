%thead
  %tr
    %th First Name
    %th Last Name
    %th Location
    %th Requests
%tbody
  - rows.group_by(&:user).each do |user, os|
    %tr.order.link{ "data-link" => new_user_response_path(user) }
      %td= user.first_name
      %td= user.last_name
      %td= user.location
      %td= render partial: "group", locals: { user: user, orders: os }
